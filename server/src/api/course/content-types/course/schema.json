    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "text",
      "maxLength": 150
    },
    "Price": {
      "type": "integer"
    },
    "realprice": {
      "type": "integer"
    },
    "Promotepic": {
