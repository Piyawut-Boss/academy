    "Detail": {
      "type": "text"
    },
    "CourseDuration": {
      "type": "integer"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "courses"
    },
    "payment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::payment.payment",
